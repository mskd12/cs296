set xlabel "number of iterations"
set ylabel "time"
set xtics 0,2
set key left top
plot '../data/plot_data.dat' using 0:1 title "steptime" with lines, \
'../data/plot_data.dat' using 0:2 title "collision time" with lines, \
'../data/plot_data.dat' using 0:3 title "velocity update time" with lines, \
'../data/plot_data.dat' using 0:4 title "position update time" with lines, \
'../data/plot_data.dat' using 0:(($2)+($3)+($4)) title "sum of collision,position,velocity times" with lines

pause -1
